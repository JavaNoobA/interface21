<!DOCTYPE beans PUBLIC  "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

    <bean id="inheritedTestBean" class="org.summerframework.beans.TestBean">
        <property name="name">
            <value>parent</value>
        </property>
        <property name="age">
            <value>1</value>
        </property>
    </bean>

    <bean id="inheritedTestBeanPrototype"
          class="org.summerframework.beans.TestBean"
          singleton="false"
    >
        <property name="name">
            <value>parent</value>
        </property>
        <property name="age">
            <value>2</value>
        </property>
    </bean>

</beans>
