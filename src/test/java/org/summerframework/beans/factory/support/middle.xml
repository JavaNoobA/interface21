<!DOCTYPE beans PUBLIC  "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

    <!-- Simple target -->
    <bean id="test" class="org.summerframework.beans.TestBean">
        <property name="name">
            <value>custom</value>
        </property>
        <property name="age">
            <value>666</value>
        </property>
    </bean>


    <!--
        Check that invoker is automatically added to wrap target.
        Non pointcut bean name should be wrapped in invoker.
    -->
    <bean id="numberTestBean"
          class="org.summerframework.beans.NumberTestBean"
    >


    </bean>


</beans>		
	
	